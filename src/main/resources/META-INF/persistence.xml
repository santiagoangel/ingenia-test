<persistence version="2.0"
             xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                       http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="ingenia">
  <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
    <jta-data-source>mysql</jta-data-source>
    <class>com.github.santiagoangel.ingenia.App</class>
    <class>com.github.santiagoangel.ingenia.Token</class>
    <class>com.github.santiagoangel.ingenia.User</class>
    
  </persistence-unit>
</persistence>
